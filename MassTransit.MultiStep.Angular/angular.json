{
  "$schema": "./node_modules/@angular-devkit/core/src/workspace/workspace-schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "masstransit-multistep-angular": {
      "root": "",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "ClientApp/index.html",
            "main": "ClientApp/main.ts",
            "polyfills": "ClientApp/polyfills.ts",
            "tsConfig": "ClientApp/tsconfig.app.json",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "ClientApp",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "ClientApp/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              {
                "input": "ClientApp/styles.css"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "ClientApp/environments/environment.ts",
                  "with": "ClientApp/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "masstransit-multistep-angular:build",
            "port": 4200
          },
          "configurations": {
            "production": {
              "browserTarget": "masstransit-multistep-angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "masstransit-multistep-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "ClientApp/test.ts",
            "polyfills": "ClientApp/polyfills.ts",
            "tsConfig": "ClientApp/tsconfig.spec.json",
            "karmaConfig": "ClientApp/karma.conf.js",
            "styles": [
              {
                "input": "styles.css"
              }
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "ClientApp/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "ClientApp/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "ClientApp/tsconfig.app.json",
              "ClientApp/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "masstransit-multistep-angular-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "masstransit-multistep-angular:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  }
}
